{
    "name": "@luminnus/lia-runtime",
    "version": "1.0.0",
    "private": true,
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./live": {
            "types": "./dist/live/index.d.ts",
            "import": "./dist/live/index.mjs",
            "require": "./dist/live/index.js"
        },
        "./contracts": {
            "types": "./dist/contracts/index.d.ts",
            "import": "./dist/contracts/index.mjs",
            "require": "./dist/contracts/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.mjs",
            "require": "./dist/utils/index.js"
        },
        "./version": {
            "types": "./dist/version/index.d.ts",
            "import": "./dist/version/index.mjs",
            "require": "./dist/version/index.js"
        },
        "./memory": {
            "types": "./dist/memory/index.d.ts",
            "import": "./dist/memory/index.mjs",
            "require": "./dist/memory/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src/",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@google/genai": "^1.0.0",
        "@luminnus/shared": "workspace:*"
    },
    "devDependencies": {
        "rimraf": "^5.0.5",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {
        "socket.io-client": "^4.0.0"
    }
}